layer {
    name : "conv1"
    type : "Convolution"
    bottom : "data"
    top : "conv1"
    param {
        lr_mult: 1
    } 
    param {
        lr_mult: 2
    } 
    convolution_param {
        num_output: 8
        pad : 1
        kernel_size : 3
        weight_filler {
            type : "msra"
