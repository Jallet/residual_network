name : "PlainNet-cifar"
layer {
  name: "cifar"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    mean_file: "/home/liangjiang/code/residual_network/data/cifar10/mean.binaryproto"
  }
  data_param {
    source: "/home/liangjiang/code/residual_network/data/cifar10/cifar10_train_lmdb"
    batch_size: 128
    backend: LMDB
  }
}
