name : "ResNet-cifar"
layer {
  name: "conv3_2"
  type: "Data"
  top: "conv3_2"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    mirror : true
  }
  data_param {
    source: "data/DyResNet-cifar6/DyResNet_conv3_2_train_lmdb"
    batch_size: 250
    backend: LMDB
  }
}
layer {
  name: "conv3_2"
  type: "Data"
  top: "conv3_2"
  top: "label"
  include {
    phase: TEST
  }
  data_param {
    source: "data/DyResNet-cifar6/DyResNet_conv3_2_test_lmdb"
    batch_size: 100
    backend: LMDB
  }
}

layer {
	bottom: "conv3_2"
	top: "conv3_2"
	name: "conv3_2_relu"
	type: "ReLU"
}
layer {
    name : "conv3_3a"
    type : "Convolution"
#    bottom : "conv3_in"
    bottom : "conv3_2"
    top : "conv3_3a"
    param {
      lr_mult : 1
    }
    param {
      lr_mult : 2
    }
    convolution_param {
        weight_filler {
            type : "msra"
        }
        bias_filler {
            type : "constant"
            value : 0
        }
        num_output : 32
        kernel_size : 3
        pad : 1
        stride : 1
    }
}
layer {
	bottom: "conv3_3a"
	top: "conv3_3a"
	name: "bn_conv3_3a"
	type: "BatchNorm"
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
}

layer {
	bottom: "conv3_3a"
	top: "conv3_3a"
	name: "scale_conv3_3a"
	type: "Scale"
    param {
        lr_mult : 1
    }
    param {
        lr_mult : 2
    }
	scale_param {
		bias_term: true
	}
}
layer {
	bottom: "conv3_3a"
	top: "conv3_3a"
	name: "conv3_3a_relu"
	type: "ReLU"
}
layer {
    name : "conv3_3b"
    type : "Convolution"
    bottom : "conv3_3a"
    top : "conv3_3b"
    param {
      lr_mult : 1
    }
    param {
      lr_mult : 2
    }
    convolution_param {
        weight_filler {
            type : "msra"
        }
        bias_filler {
            type : "constant"
            value : 0
        }
        num_output : 32
        kernel_size : 3
        pad : 1
        stride : 1
    }
}
layer {
	bottom: "conv3_3b"
	top: "conv3_3b"
	name: "bn_conv3_3b"
	type: "BatchNorm"
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
}

layer {
	bottom: "conv3_3b"
	top: "conv3_3b"
	name: "scale_conv3_3b"
	type: "Scale"
    param {
        lr_mult : 1
    }
    param {
        lr_mult : 2
    }
	scale_param {
		bias_term: true
	}
}

layer {
    type : "Eltwise"
    name : "conv3_3"
    top : "conv3_3"
    bottom : "conv3_3b"
    bottom : "conv3_2"
}

layer {
	bottom: "conv3_3"
	top: "conv3_3"
	name: "conv3_3_relu"
	type: "ReLU"
}

layer {
    bottom : "conv3_3"
    top : "conv3_sub"
    type : "Convolution"
    name : "conv3_sub"
    param {
      lr_mult : 1
    }
    param {
      lr_mult : 2
    }
    convolution_param {
        weight_filler {
            type : "msra"
        }
        bias_filler {
            type : "constant"
            value : 0
        }
        num_output : 64
        kernel_size : 1
        stride : 2
    }

}
layer {
	bottom: "conv3_sub"
	top: "conv3_sub"
	name: "bn_conv3_sub"
	type: "BatchNorm"
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
}

layer {
	bottom: "conv3_sub"
	top: "conv3_sub"
	name: "scale_conv3_sub"
	type: "Scale"
    param {
        lr_mult : 1
    }
    param {
        lr_mult : 2
    }
	scale_param {
		bias_term: true
	}
}

layer {
    name : "conv4_1a"
    type : "Convolution"
    bottom : "conv3_3"
    top : "conv4_1a"
    param {
      lr_mult : 1
    }
    param {
      lr_mult : 2
    }
    convolution_param {
        weight_filler {
            type : "msra"
        }
        bias_filler {
            type : "constant"
            value : 0
        }
        num_output : 64
        kernel_size : 1
        pad : 0
        stride : 2
    }
}
layer {
	bottom: "conv4_1a"
	top: "conv4_1a"
	name: "bn_conv4_1a"
	type: "BatchNorm"
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
}

layer {
	bottom: "conv4_1a"
	top: "conv4_1a"
	name: "scale_conv4_1a"
	type: "Scale"
    param {
        lr_mult : 1
    }
    param {
        lr_mult : 2
    }
	scale_param {
		bias_term: true
	}
}
layer {
	bottom: "conv4_1a"
	top: "conv4_1a"
	name: "conv4_1a_relu"
	type: "ReLU"
}
layer {
    name : "conv4_1b"
    type : "Convolution"
    bottom : "conv4_1a"
    top : "conv4_1b"
    param {
      lr_mult : 1
    }
    param {
      lr_mult : 2
    }
    convolution_param {
        weight_filler {
            type : "msra"
        }
        bias_filler {
            type : "constant"
            value : 0
        }
        num_output : 64
        kernel_size : 3
        pad : 1
        stride : 1
    }
}
layer {
	bottom: "conv4_1b"
	top: "conv4_1b"
	name: "bn_conv4_1b"
	type: "BatchNorm"
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
    param {
        lr_mult : 0
    }
}

layer {
	bottom: "conv4_1b"
	top: "conv4_1b"
	name: "scale_conv4_1b"
	type: "Scale"
    param {
        lr_mult : 1
    }
    param {
        lr_mult : 2
    }
	scale_param {
		bias_term: true
	}
}

layer {
    type : "Eltwise"
    name : "conv4_1"
    top : "conv4_1"
    bottom : "conv3_sub"
    bottom : "conv4_1b"
}

layer {
	bottom: "conv4_1"
	top: "conv4_1"
	name: "conv4_1_relu"
	type: "ReLU"
}
layer {
    name : "global_pool"
    bottom : "conv4_1"
#    bottom : "conv4_1b"
    top : "global_pool"
    type : "Pooling"
    pooling_param {
        kernel_size : 8
        stride : 8
        pool : AVE
    }
}

layer {
    name : "ip"
    bottom : "global_pool"
    top : "ip"
    type : "InnerProduct"
    param {
        lr_mult : 1
    }
    param {
        lr_mult : 2
    }
    inner_product_param {
        num_output : 10
    }
}

layer {
    name : "accuracy"
    type : "Accuracy"
    bottom : "ip"
    bottom : "label"
    top : "accuracy"
}

layer {
    name : "conv4_1b_rc"
    bottom : "conv4_1b"
    top : "conv4_1b_rc"
    type : "Deconvolution"
    param {
        lr_mult : 1
    }
    param {
        lr_mult : 2
    }
    convolution_param {
        weight_filler {
            type : "msra"
        }
        bias_filler {
            type : "constant"
            value : 0
        }
        num_output : 64
        kernel_size : 3
        pad : 1
        stride : 1
    }
}
layer {
    name : "loss_classify"
    bottom : "ip"
    bottom : "label"
    top : "loss_classify"
    type : "SoftmaxWithLoss"
    loss_weight : 1
}

layer {
    name : "loss_rc"
    bottom : "conv4_1b_rc"
    bottom : "conv3_sub"
    top : "loss_rc"
    type : "EuclideanLoss"
    loss_weight : 1e-4
}

layer {
    name : "loss"
    bottom : "loss_classify"
    bottom : "loss_rc"
    top : "loss"
    type : "Eltwise"
}
