      lr_mult: 1
    }
    param {
      lr_mult: 2
    }
    convolution_param {
        weight_filler {
            type : "msra"
        }
        bias_filler {
            type : "constant"
            value : 0
        }
        num_output : 16
        kernel_size : 3
        pad : 1
        stride : 1
    }
}
layer {
	bottom: "conv2_1a"
