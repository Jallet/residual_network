}

layer {
    name : "ip"
    bottom : "global_pool"
    top : "ip"
    type : "InnerProduct"
    inner_product_param {
        num_output : 10
    }
}

layer {
    name : "accuracy"
    type : "Accuracy"
    bottom : "ip"
    bottom : "label"
    top : "accuracy"
}

layer {
    name : "loss"
    bottom : "ip"
    bottom : "label"
    top : "loss"
    type : "SoftmaxWithLoss"
}
